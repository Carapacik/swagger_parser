                      }
                    }
                  },
                  "name": {
                    "type": "string",
                    "nullable": true
                  },
                  "image": {
                    "description": "Sample Image",
                    "type": "string",
                    "format": "binary",
                    "nullable": true
                  }
                },
                "required": [
                  "files",
                  "name"
